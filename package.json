{
  "private": true,
  "scripts": {
    "build:hooks": "turbo run dev --filter=@oncepal/hooks",
    "dev:ui": "turbo run dev --filter=@opui/react",
    "dev:native": "turbo run dev --filter=@oncepal/native",
    "dev:h5": "turbo run dev --filter=@oncepal/h5",
    "dev:admin": "turbo run dev --filter=@oncepal/admin",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore"
  },
  "devDependencies": {
    "prettier": "^3.1.1",
    "turbo": "^2.0.3"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=18"
  },
  "name": "oncepal-web"
}
